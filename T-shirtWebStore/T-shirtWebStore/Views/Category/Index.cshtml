@model List<Category>
<h1>Category</h1>
<div>
    <a asp-controller="Category" asp-action="Create" class="btn btn-primary">
        Create New Category
    <i class="bi bi-plus-lg"></i>
    </a>
</div>
<table class="table table-borderless">
    <thead>
        <tr>  
            <th>
                Category Name 
            </th>
            <th>
                Display Order 
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
        {
            <tr>
                <td>@obj.Name</td>
            <td>@obj.DisplayOrder</td>
        </tr>
        }
    </tbody>
</table>